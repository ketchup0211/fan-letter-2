{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@assets/*" : ["src/assets/*"],
      "@pages/*" : ["src/pages/*"],
      "@redux/*" : ["src/redux/*"],
      "@shared/*" : ["src/shared/*"],
      "@util/*": ["src/util/*"],
      "@context/*" : ["src/context/*"]
    }
  },
  "include": ["src", "**/*.js", "**/*.jsx"],
}
